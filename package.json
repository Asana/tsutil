{
  "name": "tsutil",
  "version": "4.0.1",
  "description": "TypeScript Utility Data Structures",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebundle": "npm run compile && mv _build/src/** .",
    "bundle": "dts-generator --name tsutil --main index.d.ts --baseDir . --exclude node_modules/dts-generator/node_modules/typescript/bin/lib.es6.d.ts --out tsutil.d.ts  *.d.ts",
    "postbundle": "find . -type f -name '*.d.ts' -maxdepth 1 | grep --invert-match 'tsutil.d.ts' | xargs rm",
    "clean": "rm -rf _build coverage",
    "precompile": "npm run clean",
    "compile": "tsc",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "find src test -name \"*.ts\" | sed 's/^/--file=/g' | xargs tslint",
    "pretest": "npm run compile & npm run lint",
    "test": "istanbul cover _mocha -- --reporter ${MOCHA_REPORTER-nyan} --slow 10 --ui bdd --recursive _build/test",
    "posttest": "istanbul check-coverage --statements 100 --branches 95 --functions 100 --lines 100",
    "typings": "tsd reinstall && tsd rebundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Asana/tsutil.git"
  },
  "keywords": [
    "asana",
    "typescript",
    "util",
    "data",
    "structures",
    "collections"
  ],
  "contributors": [
    {
      "name": "Daniel Phan",
      "email": "daniel@asana.com"
    },
    {
      "name": "Phips Peter",
      "email": "phips@asana.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Asana/tsutil/issues"
  },
  "homepage": "https://github.com/Asana/tsutil",
  "devDependencies": {
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "dts-generator": "^1.4.1",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1",
    "tsd": "^0.6.0-beta.5",
    "tslint": "^2.2.0-beta",
    "typescript": "^1.5.0-beta"
  },
  "typescript": {
    "definition": "tsutil.d.ts"
  }
}
