{
  "name": "tsutil",
  "version": "4.0.1",
  "description": "TypeScript Utility Data Structures",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "bundle": "npm run clean && npm run compile && mv _build/src/** . && node _build/tools/bundle.js",
    "clean": "rm -rf _build coverage doc",
    "compile": "tsc",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "find src test -name \"*.ts\" | sed 's/^/--file=/g' | xargs tslint",
    "pretest": "npm run compile -- --sourceMap & npm run lint",
    "test": "istanbul cover _mocha -- --reporter $npm_package_config_reporter --slow 10 --ui bdd --recursive _build/test",
    "posttest": "istanbul check-coverage --statements 100 --branches 95 --functions 100 --lines 100",
    "typings": "tsd reinstall && tsd rebundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Asana/tsutil.git"
  },
  "keywords": [
    "asana",
    "typescript",
    "util",
    "data",
    "structures",
    "collections"
  ],
  "contributors": [
    {
      "name": "Daniel Phan",
      "email": "daniel@asana.com"
    },
    {
      "name": "Phips Peter",
      "email": "phips@asana.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Asana/tsutil/issues"
  },
  "homepage": "https://github.com/Asana/tsutil",
  "devDependencies": {
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "dts-bundle": "^0.2.0",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1",
    "source-map-support": "^0.2.10",
    "tsd": "^0.6.0-beta.5",
    "tslint": "^2.2.0-beta",
    "typescript": "^1.5.0-beta",
    "typescript-formatter": "^0.3.2"
  },
  "typescript": {
    "definition": "tsutil.d.ts"
  },
  "config": {
    "reporter": "nyan"
  }
}
